{
  "name": "turboapp",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "publish:ui": "pnpm --filter @ngunawan/ui publish"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.1",
      "react-dom": "19.1.1",
      "@types/react": "19.1.1",
      "@types/react-dom": "19.1.1",
      "react-test-renderer": "19.1.1",
      "react-native": "0.82.1"
    }
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
